<h1>Listing supplies</h1>

<table>
  <tr>
    <th>Supply count</th>
    <th>Created at</th>
    <th>Updated at</th>
    <th>Product</th>
    <th>Shop</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @supplies.each do |supply| %>
  <tr>
    <td><%= supply.supply_count %></td>
    <td><%= supply.created_at %></td>
    <td><%= supply.updated_at %></td>
    <td><%= supply.product_id %></td>
    <td><%= supply.shop_id %></td>
    <td><%= link_to 'Show', supply %></td>
    <td><%= link_to 'Edit', edit_supply_path(supply) %></td>
    <td><%= link_to 'Destroy', supply, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Supply', new_supply_path %>
